<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eku.dry_ticket_project" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.first_activity"
            android:icon="@drawable/drylogo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.sign_in"
            android:icon="@drawable/d_logo"
            android:label="Sign In" />
        <activity
            android:name=".activity.sign_up"
            android:icon="@drawable/d_logo"
            android:label="Sign Up" />
        <activity
            android:name=".activity.NowOnSale"
            android:icon="@drawable/d_logo"
            android:label="Now On Sale" />
        <activity
            android:name=".activity.UpcomingEvents"
            android:icon="@drawable/d_logo"
            android:label="Upcoming Events" />
        <activity
            android:name=".activity.PastEvents"
            android:icon="@drawable/d_logo"
            android:label="Past Events" />
        <activity
            android:name=".activity.Venue"
            android:icon="@drawable/d_logo"
            android:label="Venues" />
 >
        <activity
            android:name=".activity.singleitemview"
            android:icon="@drawable/d_logo" />
        <activity
            android:name=".activity.single_venue"
            android:icon="@drawable/d_logo" />
        <activity
            android:name=".activity.Ticket_Booking"
            android:icon="@drawable/d_logo" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".activity.ContactUs"
            android:icon="@drawable/d_logo"
            android:label="@string/title_activity_maps" >
        </activity>
        <activity
            android:name=".activity.venue_map"
            android:label="@string/title_activity_venue_map" >
        </activity>
        <activity android:name=".activity.Artist_information" android:label="Artists"/>
        <activity android:name=".activity.Artist_detail_information"/>
        <activity android:name=".activity.Seat_allocation"></activity>

    </application>

</manifest>
